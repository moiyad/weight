<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tr2355.weighttracker">

    <application
        android:name=".AppConfig"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />


                <meta-data android:name="DATABASE" android:value="tracker.db" />
                <meta-data android:name="VERSION" android:value="3" />
                <meta-data android:name="QUERY_LOG" android:value="false" />
                <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.example.tr2355.weighttracker" />

            </intent-filter>

        </activity>
        <activity android:name=".demo" android:parentActivityName=".MainActivity"></activity>

    </application>

</manifest>